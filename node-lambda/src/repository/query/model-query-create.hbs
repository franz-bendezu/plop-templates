export const CREATE_QUERY = `
INSERT INTO {{snakeCase name}}
                 (NAME, DESCRIPTION, VALID_FROM, VALID_TO{{#each rowProperties}}, {{snakeCase name}}{{/each}})
                  VALUES (NAME, DESCRIPTION, VALID_FROM, VALID_TO{{#each rowProperties}}, ${{add @index 5}}{{/each}})
                  RETURNING ID, NAME, DESCRIPTION, VALID_FROM, VALID_TO,
                  CREATED_AT, MODIFIED_AT, CREATED_BY, MODIFIED_BY
{{#each rowProperties}}                  , {{snakeCase name}}{{/each}}
    `;
